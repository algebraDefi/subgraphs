{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^codegen", "^build"],
      "outputs": ["build/**"]
    },
    "codegen": {
      "dependsOn": ["^codegen"],
      "outputs": ["build/**", "generated/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
